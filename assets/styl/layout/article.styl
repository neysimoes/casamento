p, h1, h2, h3, h4, h5, h6
  padding-bottom 10px
h1, h2, h3, h4, h5, h6
  font-weight 900

p
  font-size 1.1rem
  line-height 2rem
  padding-bottom 2rem
em
  margin 0 auto
.music
  text-align center
  em
    text-align left
    display inline-block
    width 300px
.article
  padding: 5rem 0;

.article-title
  color #fff
  font-size 3rem
  padding 3rem 0
  text-align center
  text-transform uppercase

.article-subtitle
  color #fff
  text-align center
  font-weight 400

.jumbo-image
  display block
  position relative
  height 100%
  background-repeat no-repeat
  background-size cover
  background-position center center
